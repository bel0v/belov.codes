{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "paths": {
      "config/*": ["./src/config/*"],
      "components/*": ["./src/components/*"],
      "layouts/*": ["./src/layouts/*"],
      "assets/*": ["./src/assets/*"],
      "styles/*": ["./src/styles/*"]
    }
  }
}
